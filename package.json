    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3"
  },
  "devDependencies": {
    "body-parser": "^1.20.0",
    "c8": "^7.11.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.2.2"
  }
