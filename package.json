{
  "name": "hugin-cache",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha tests --recursive --exclude tests/k6 --timeout 10000 --exit",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --seeders-path ./database/seeders",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kryptokrona/hugin-cache.git"
  },
  "keywords": [
    "kryptokrona",
    "hugin"
  ],
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kryptokrona/hugin-cache/issues"
  },
  "homepage": "https://github.com/kryptokrona/hugin-cache",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.2",
    "http-errors": "^2.0.0",
    "hugin-crypto": "^0.0.2",
    "loglevel": "^1.8.0",
    "morgan": "^1.10.0",
    "perf_hooks": "^0.0.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
